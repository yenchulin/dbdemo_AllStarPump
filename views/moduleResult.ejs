<% include ./header %>

    <div class=row>
        <div class=col-lg-4>
            <h2>模具</h2>
            <table id="module" border=1 bordercolor=#003366 width=900>
                <tr>
                    <th>模具機型</th>
                    <th>模具用途</th>
                    <th>模具數量</th>
                    <th>廠商A名稱</th>
                    <th>原有數量</th>
                </tr>
                <form action="/query/moduleResult" method="POST" novalidate>

                <tr>
                    <th name="moduleId">
                        <%= module.moduleId %>
                    </th>
                    <th>
                        <%= module.moduleUsage %>
                    </th>
                    <th>
                        <a href="#" id="moduleQuantity" data-type="text" data-pk="1" data-url="/post" data-title="update quantity">
                            <%= module.moduleQuantity %>
                        </a>
                    </th>
                    <th>
                        <a href="#" id="acompanyName" data-type="text" data-pk="1" data-url="/post" data-title="update name">
                            <%= module.acompanyName %>
                        </a>
                    </th>
                    <th>
                        <a href="#" id="quantityOwned" data-type="text" data-pk="1" data-url="/post" data-title="update quantity">
                            <%= module.quantityOwned %>
                        </a>
                    </th>
                </tr>
            </table>
            <input type="submit" value="刪除">

        </div>
    </div>

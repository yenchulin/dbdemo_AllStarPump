<% include ./header %>
    <form action="/query/moduleResult" method="POST" novalidate>
        <div class=row>
            <div class=col-lg-4>
                <h2>模具</h2>
                <form>模具機型:
                    <select name="moduleId">
                        <% moduleList.forEach(function(module) { %>
                            <option>
                                <%= module.moduleId %>
                            </option>
                            <%  }); %>
                    </select>
                </form>
                <form>模具用途:
                    <select name="moduleUsage">
                        <% moduleList.forEach(function(module) { %>
                            <option>
                                <%= module.moduleUsage %>
                            </option>
                            <%  }); %>
                    </select>
                </form>
                <form>目前位置:
                    <select name="currentLocation">
                        <% moduleList.forEach(function(module) { %>
                            <option>
                                <%= module.currentLocation %>
                            </option>
                            <%  }); %>
                    </select>
                </form>
                <input type="submit" value="送出">
            </div>
    </form>
    <form action="/query/AcomResult" method="POST" novalidate>
        <div class=row>
            <div class=col-lg-4>
                <h2>廠商A</h2>
                <form>廠商名稱:
                    <select name="Aname">>
                        <% acomList.forEach(function(acompany) { %>
                            <option>
                                <%= acompany.firmName %>
                            </option>
                            <%  }); %>
                    </select>
                </form>
                <form>負責人:
                    <select name="Aperson">>
                        <% acomList.forEach(function(acompany) { %>
                            <option>
                                <%= acompany.personIC %>
                            </option>
                            <%  }); %>
                    </select>
                </form>
                <form>電話號碼:
                    <select name="AphoneNo">>
                        <% acomList.forEach(function(acompany) { %>
                            <option>
                                <%= acompany.phoneNo %>
                            </option>
                            <%  }); %>
                    </select>
                </form>
                <form>零件代碼:
                    <select name="componentId">>
                        <% acomList.forEach(function(acompany) { %>
                            <option>
                                <%= acompany.componentId %>
                            </option>
                            <%  }); %>
                    </select>
                </form>
                <form>零件名稱:
                    <select name="componentName">>
                        <% acomList.forEach(function(acompany) { %>
                            <option>
                                <%= acompany.componentName %>
                            </option>
                            <%  }); %>
                    </select>
                </form>

                <form>廠商A地址:
                    <select name="Aaddress">>
                        <% acomList.forEach(function(acompany) { %>
                            <option>
                                <%= acompany.address %>
                            </option>
                            <%  }); %>
                    </select>
                </form>
                <form>借出時間:
                    <select name="rentTime">>
                        <% acomList.forEach(function(acompany) { %>
                            <option>
                                <%= acompany.lendOT %>
                            </option>
                            <%  }); %>
                    </select>
                </form>
                <form>歸還時間:
                    <select name="returnTime">>
                        <% acomList.forEach(function(acompany) { %>
                            <option>
                                <%= acompany.returnT %>
                            </option>
                            <%  }); %>
                    </select>
                </form>
                <input type="submit" value="送出">
            </div>
        </div>
    </form>
    <form action="/query/BcomResult" method="POST" novalidate>
